<filetype binary="false" description="Makefile" name="Makefile">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="#" />
      <option name="COMMENT_START" value="" />
      <option name="COMMENT_END" value="" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="false" />
    </options>
    <keywords keywords="ifeq;ifneq;else;endif;define;endef;ifdef;ifndef" ignore_case="true" />
    <keywords2 keywords=".default_goal;.features;.include_dirs;.phony;.recipeprefix;.variables;all;clean;mostlyclean;distclean;realclean;clobber;install;print;tar;shar;dist;TAGS;check;test" />
    <keywords3 keywords="foreach;call;eval;include;subst;patsubst;strip;findstring;filter;filter-out;sort;word;wordlist;words;firstword;lastword;dir;notdir;suffix;basename;addsuffix;addprefix;join;wildcard;realpath;abspath;or;and;file;origin;flavor;error;warning;info;shell;" />
  </highlighting>
  <extensionMap>
    <mapping pattern="GNUmakefile" />
    <mapping pattern="Makefile" />
    <mapping pattern="makefile" />
    <mapping pattern="*.mk" />
    <mapping pattern="gmsl" />
    <mapping pattern="__gmsl" />
  </extensionMap>
</filetype>
